Cell[CellGroupData[{

Cell["Symmetrizing the model while loading it", "Section",
 CellChangeTimes->{{3.9242402829081573`*^9, 
  3.92424029651335*^9}},ExpressionUUID->"5f02c358-6a2c-42f6-b5e9-\
ccd0a8c33c32"],

Cell["\<\
To see the effect of the symmetrisation, we first observe the model without \
the staggered chemical potential, which we save to another file:\
\>", "Text",
 CellChangeTimes->{{3.924240331848802*^9, 3.9242403663865757`*^9}, {
  3.92424049955691*^9, 
  3.9242405055388803`*^9}},ExpressionUUID->"c274324d-2e21-4967-8920-\
48392bac020a"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Export", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"NotebookDirectory", "[", "]"}], "<>", "\"\<tbm2.dat\>\""}], ",", 
    RowBox[{"tbData", "[", 
     RowBox[{"1", ",", "0.25", ",", "0"}], "]"}], ",", "\"\<CSV\>\""}], "]"}],
   ";"}]], "Input",
 CellChangeTimes->{{3.9242403731767187`*^9, 3.924240375892815*^9}},
 CellLabel->"In[31]:=",ExpressionUUID->"bc954c8a-82b0-4fb7-a320-e2c7994874b1"],

Cell["\<\
We can load the new model, setting messages to false \
(\[OpenCurlyDoubleQuote]Messages\[CloseCurlyDoubleQuote]\[Rule]False) to \
avoid printing comments.\
\>", "Text",
 CellChangeTimes->{{3.924240508179885*^9, 3.92424051563291*^9}, {
  3.924240556327065*^9, 
  3.924240615391526*^9}},ExpressionUUID->"9fdb1ea4-2519-45f9-9027-\
506bd17c1bdc"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Hamltn2", "[", 
    RowBox[{"{", 
     RowBox[{"k1_", ",", "k2_"}], "}"}], "]"}], "=", 
   RowBox[{
    RowBox[{"LoadTBM", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"NotebookDirectory", "[", "]"}], "<>", "\"\<tbm2.dat\>\""}], 
      ",", 
      RowBox[{"\"\<LatticeVectors\>\"", "->", "LattVecs"}], ",", 
      RowBox[{"\"\<Messages\>\"", "->", "False"}]}], "]"}], "[", 
    RowBox[{"[", "1", "]"}], "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.924240390921074*^9, 3.924240411779078*^9}, {
  3.927026990697443*^9, 3.9270269921517*^9}},
