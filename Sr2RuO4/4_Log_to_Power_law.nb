Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"LogPowerTrans", "=", 
   RowBox[{"{", "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SetSharedVariable", "[", "LogPowerTrans", "]"}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{"Do", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"HamCurr", "[", "k_", "]"}], ":=", 
     RowBox[{"Hamltn1", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"k", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "-", 
        RowBox[{"k", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], ",", 
       RowBox[{
        RowBox[{"k", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "+", 
        RowBox[{"k", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], ",", "\[Theta]curr"}], "]"}]}], 
    ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"Tayl", "=", 
     RowBox[{"TaylorExpandBand", "[", 
      RowBox[{"HamCurr", ",", 
       RowBox[{"{", 
        RowBox[{"0", ",", 
         RowBox[{"\[Pi]", "//", "N"}]}], "}"}], ",", "4", ",", "4", ",", 
       RowBox[{"\"\<Messages\>\"", "->", "False"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Abs", "[", 
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"Tayl", "[", 
           RowBox[{"[", 
            RowBox[{"1", ",", "5"}], "]"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"p1", ",", "4"}], "}"}]}], "]"}], "]"}], ">", 
       RowBox[{"Abs", "[", 
        RowBox[{"D", "[", 
         RowBox[{
          RowBox[{"Tayl", "[", 
           RowBox[{"[", 
            RowBox[{"1", ",", "5"}], "]"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"p2", ",", "4"}], "}"}]}], "]"}], "]"}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"TaylTrans", "=", 
        RowBox[{"Tayl", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"TaylTrans", "=", 
        RowBox[{"Tayl", "[", 
         RowBox[{"[", "2", "]"}], "]"}]}], ";"}]}], "\[IndentingNewLine]", 
     "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"Etrans", "=", 
     RowBox[{"1000", 
      RowBox[{"(", 
       RowBox[{"Abs", "[", 
        RowBox[{
         RowBox[{
          SuperscriptBox[
           RowBox[{"D", "[", 
            RowBox[{
             RowBox[{"TaylTrans", "[", 
              RowBox[{"[", "3", "]"}], "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"p1", ",", "2"}], "}"}]}], "]"}], "2"], "/", 
          RowBox[{"D", "[", 
           RowBox[{
            RowBox[{"TaylTrans", "[", 
             RowBox[{"[", "5", "]"}], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"p1", ",", "4"}], "}"}]}], "]"}]}], 
         RowBox[{
          RowBox[{"Factorial", "[", "4", "]"}], "/", "4"}]}], "]"}], 
       ")"}]}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
     RowBox[{
     "\"\<\[Theta] = \>\"", ",", "\[Theta]curr", ",", 
      "\"\<\[Degree], Tayl = \>\"", ",", 
      RowBox[{"NumberForm", "[", 
       RowBox[{
        RowBox[{"Chop", "[", 
         RowBox[{
          RowBox[{"Total", "[", 
           RowBox[{"TaylTrans", "/.", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"p1", "->", 
               SubscriptBox["k", "x"]}], ",", 
              RowBox[{"p2", "->", 
               SubscriptBox["k", "y"]}]}], "}"}]}], "]"}], ",", 
          SuperscriptBox["10", 
           RowBox[{"-", "6"}]]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"2", ",", "3"}], "}"}]}], "]"}], ",", 
      "\"\<\\n\\t |\!\(\*SubscriptBox[\(\[Epsilon]\), \(trans\)]\) - \
\!\(\*SubscriptBox[\(\[Epsilon]\), \(VHS\)]\)| = \>\"", ",", 
      RowBox[{"NumberForm", "[", 
       RowBox[{"Etrans", ",", 
        RowBox[{"{", 
         RowBox[{"3", ",", "2"}], "}"}]}], "]"}], ",", "\"\< meV.\>\""}], 
     "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"AppendTo", "[", 
     RowBox[{"LogPowerTrans", ",", 
      RowBox[{"{", 
       RowBox[{"\[Theta]curr", ",", "Etrans"}], "}"}]}], "]"}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", ",", 
   RowBox[{"{", 
    RowBox[{"\[Theta]curr", ",", "7", ",", "12", ",", "0.5"}], "}"}]}], 
  "]"}]}], "Input",
 CellChangeTimes->{{3.9276171743475113`*^9, 3.92761718786701*^9}, {
   3.927617228049974*^9, 3.927617279667111*^9}, {3.9276173150022717`*^9, 
   3.927617426588004*^9}, {3.927617659515403*^9, 3.927617752972859*^9}, {
   3.927617890449483*^9, 3.927617923033194*^9}, {3.9276179779654837`*^9, 
